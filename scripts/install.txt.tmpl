repository: https://github.com/{{ .User }}/{{ .Program }}
user: {{ .User }}
program: {{ .Program }}{{if .AsProgram }}
as: {{ .AsProgram }}{{end}}
release: {{ .Release }}
move-into-path: {{ .MoveToPath }}
private: {{ .Private }}
platform: {{ .Platform }}

release assets:
{{ range .Assets }}  {{ .Key }}
    url:    {{ .URL }}
{{end}}
has-m1-asset: {{ .M1Asset }}

